{
  "definitions": {}, 
  "$schema": "http://json-schema.org/draft-06/schema#", 
  "type": "object", 
  "id": " ", 
  "additionalProperties": false, 
  "required": [
    "data"
  ], 
  "properties": {
    "data": {
      "type": "object", 
      "id": "/properties/data", 
      "additionalProperties": false, 
      "required": [
        "author", 
        "title", 
        "url", 
        "username"
      ], 
      "properties": {
        "author": {
          "type": "string", 
          "id": "/properties/data/properties/author", 
          "minLength": 1, 
          "maxLength": 55
        }, 
        "title": {
          "type": "string", 
          "id": "/properties/data/properties/title", 
          "minLength": 1, 
          "maxLength": 55
        }, 
        "url": {
          "type": "string",
          "format": "uri",
          "id": "/properties/data/properties/url", 
          "minLength": 1, 
          "maxLength": 55
        }, 
        "username": {
          "type": "string",
          "pattern": "^[a-zA-Z0-9]*$",
          "id": "/properties/data/properties/username", 
          "minLength": 1, 
          "maxLength": 55
        }
      }
    }
  }
}